{
    "name": "lfbellante/regioni-province-comuni",
    "description": "Questo package di laravel permette d'importare tutte le regioni d'italia, le relative province e i loro comuni. Permettendo quindi di usare queste informazioni in ogni istanza necessaria.",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Luca Francesco Bellante",
            "email": "lfbellante@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^5.3.3 || ^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Lfbellante\\RegioniProvinceComuni\\": "./src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lfbellante\\RegioniProvinceComuni\\RegionsServiceProvider"
            ]
        }
    }
}
